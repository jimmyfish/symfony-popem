<body>
<div class="se-pre-con"></div>
<section class="sticky-top shadowed">
    <section id="topper-nav">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <i class="fa fa-clock-o"></i> &nbsp;
                    {{ "now"|date('l, d F Y | h:i') }}
                </div>
                <div class="col-md-2">
                    {% if (app.session.get('isLogin') is null) %}
                        <a href="#" class="sign-in">
                            <i class="fa fa-sign-in"></i> Login Account
                        </a>
                    {% else %}
                        <a href="{{ path('popem_client_dashboard') }}" >
                            <i class="fa fa-user"></i> Beranda
                        </a>
                    {% endif %}
                    <div class="content hide">
                        <p class="text-base fz-19">Login</p>
                        <form method="post" id="login-form">
                            <div class="form-group">
                                <label for="_username">Nama Pengguna : </label>
                                <input type="text" class="form-control" name="_username" id="_username">
                            </div>
                            <div class="form-group">
                                <label for="_password">Kata Sandi : </label>
                                <input type="password" class="form-control" name="_password" id="_password">
                            </div>
                            <div class="form-group">
                                <p class="notification"><label for="" class="process-text text-capitalize" style="font-size: 13px;">&nbsp;</label></p>
                                <span><a href="{{ path('popem_client_forgot_password') }}" class="text-base">Forgot password</a></span>
                                <input type="submit" value="LOGIN" class="btn red-button plr-30 pull-right">
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-md-2">
                    <a href="#">
                        <i class="fa fa-percent"></i> Bonus 100%
                    </a>
                </div>
                <div class="col-md-2">
                    <a href="#">
                        <i class="fa fa-user-circle-o"></i> Open Accounts
                    </a>
                </div>
            </div>
        </div>
    </section>
    <nav class="navbar navbar-default">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <div class="navbar-header">
                    <a class="navbar-brand" href="{{ path('popem_client_index') }}">
                        <img alt="" src="{{ asset('assets_client/img/logo.png') }}">
                    </a>
                </div>
                <button aria-expanded="false" class="navbar-toggle collapsed"
                        data-target="#bs-example-navbar-collapse-1" data-toggle="collapse" type="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    {% for file in files %}
                        <li><a href="{{ file['path'] }}">{{ file['label'] }}</a></li>
                    {% endfor %}
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Category <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            {% for menu in submenu %}
                                <li><a href="{{ menu['path'] }}">{{ menu['label'] }}</a></li>
                            {% endfor %}
                        </ul>
                    </li>
                </ul>

            </div>
            <!-- /.navbar-collapse -->
            {#{{ dump(submenu) }}#}
        </div>
        <!-- /.container-fluid -->
    </nav>
</section>

