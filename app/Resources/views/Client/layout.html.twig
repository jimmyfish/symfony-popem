<script>
    $(function () {
        $(".se-pre-con").fadeOut("slow");
        $(".notification").find(".process-text").addClass("hide"), $(document).on("submit", ".popover #login-form", function (t) {
            t.preventDefault();
            var e = {username: $("#_username").val(), password: $("#_password").val()}, s = $(this).find("input");
            s.prop("disabled", !0), $(".notification").find(".process-text").removeClass("hide"), $(this).find(".process-text").text("Processing ...").removeClass("text-danger"), $.ajax({
                type: "POST",
                url: "{{ path('popem_api_client_login') }}",
                data: e,
                success: function (t) {
                    !0 === t.data.status ? location.href = "{{ path('popem_client_dashboard') }}" : (s.prop("disabled", !1), $(".notification").find(".process-text").removeClass("hide"), $(".notification").find(".process-text").text(t.data.message).addClass("text-danger"))
                }
            })
        })
    });
</script>

{% block customJS %}{% endblock %}
